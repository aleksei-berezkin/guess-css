{
  "name": "guess-css",
  "version": "0.0.0",
  "description": "HTML & CSS puzzler game",
  "main": "src/index.tsx",
  "scripts": {
    "clean": "rm -rf ./dist",
    "clean-generated": "rm -rf ./generated",
    "generate": "ts-node --project tsconfig.build.json ./scripts/generate.ts",
    "copy-logo": "cp css-logo.png dist/",
    "dev": "next dev",
    "build": "npm-run-all clean clean-generated generate build@next copy-logo",
    "build@next": "next build",
    "start": "next start",
    "lint": "next lint",
    "--copy-index-to-paths": "ts-node --project tsconfig.build.json ./scripts/copyIndexToPaths.ts",
    "--analyze": "NODE_ENV=production webpack --config webpack.analyze.js"
  },
  "author": "Aleksei Berezkin",
  "license": "ISC",
  "repository": "https://github.com/aleksei-berezkin/guess-css",
  "devDependencies": {
    "@types/node": "^20.17.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.0.2",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.6",
    "@mui/material": "^6.1.6",
    "@mui/styles": "^6.1.6",
    "next": "^15.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-ga": "^3.3.1",
    "react-swipeable-views-react-18-fix": "^0.14.1"
  }
}
