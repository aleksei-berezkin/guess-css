{
  "name": "guess-css",
  "version": "0.0.0",
  "description": "HTML & CSS puzzler game",
  "main": "src/index.tsx",
  "scripts": {
    "clean": "rm -rf ./dist",
    "clean-generated": "rm -rf ./generated",
    "generate": "node --import 'data:text/javascript,import { register } from \"node:module\"; import { pathToFileURL } from \"node:url\"; register(\"ts-node/esm\", pathToFileURL(\"./\"));' ./scripts/generate.mts",
    "copy-logo": "cp css-logo.png dist/",
    "dev": "vite --port 3000",
    "build@vite": "tsc -b && vite build",
    "build": "npm-run-all clean clean-generated generate build@vite copy-logo",
    "preview": "vite preview",
    "--copy-index-to-paths": "ts-node --project tsconfig.build.json ./scripts/copyIndexToPaths.ts",
    "analyze": "npx vite-bundle-visualizer"
  },
  "author": "Aleksei Berezkin",
  "license": "ISC",
  "repository": "https://github.com/aleksei-berezkin/guess-css",
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@types/node": "^20.17.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.11"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.6",
    "@mui/material": "^6.1.6",
    "@mui/material-pigment-css": "^6.1.6",
    "@mui/styles": "^6.1.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-ga": "^3.3.1",
    "react-router": "^6.28.0",
    "react-router-dom": "^6.28.0",
    "react-scroll-snapper": "^0.1.0",
    "vite-bundle-visualizer": "^1.2.1"
  }
}
