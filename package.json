{
  "name": "guess-css",
  "version": "0.0.0",
  "description": "HTML & CSS puzzler game",
  "main": "src/entry-client.tsx",
  "scripts": {
    "lint": "npx eslint",
    "clean": "rm -rf ./dist",
    "clean-generated": "rm -rf ./generated",
    "generate": "tsx ./scripts/generate.mts",
    "webpack": "NODE_ENV=production webpack",
    "copy-logo": "cp css-logo.png dist/",
    "ssr": "npx tsc && node --experimental-require-module ssr.js && rm -r ./out",
    "ssr@dev": "npx tsc && node --experimental-require-module ssr.js",
    "build": "npm-run-all lint clean clean-generated generate webpack ssr copy-logo",
    "dev": "NODE_ENV=development webpack serve --port 3000",
    "preview": "serve ./dist",
    "analyze": "NODE_ENV=production webpack --config webpack.analyze.js"
  },
  "author": "Aleksei Berezkin",
  "license": "ISC",
  "repository": "https://github.com/aleksei-berezkin/guess-css",
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/node": "^20.17.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "esbuild-loader": "^4.2.2",
    "eslint": "^9.15.0",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.12.0",
    "html-webpack-plugin": "^5.6.3",
    "npm-run-all": "^4.1.5",
    "serve": "^14.2.4",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.15.0",
    "webpack": "^5.96.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.6",
    "@mui/material": "^6.1.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "wouter": "^3.3.5"
  }
}
