    <style id="loading-screen-style">
        body {
            margin: 0;
        }

        #loading-screen {
            display: flex;
            justify-content: center;
            align-items: center;
            position: fixed;
            width: 100%;
            height: 100%;
        }

        #loading-screen svg {
            animation: spinner-rotation 2s linear infinite;
        }

        #loading-screen circle {
            fill: none;
            stroke: #212121;
            stroke-width: 4px;
            stroke-dasharray: 104px;
        }

        @keyframes spinner-rotation {
            0% {
                transform: rotate(0deg);
            }
            100% {
                transform: rotate(360deg);
            }
        }
